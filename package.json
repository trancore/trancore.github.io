{
  "name": "trancore.github.io",
  "homepage": "https://trancore.github.io/",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "eslint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "stylelint": "stylelint --fix '**/*.scss'",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,scss}'",
    "precommit": "lint-staged",
    "predeploy": "yarn run build",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "framer-motion": "^12.23.24",
    "graphql": "^16.12.0",
    "modern-css-reset": "^1.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.6",
    "react-scripts": "5.0.1",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sass": "^1.94.2",
    "urql": "^5.0.1"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@graphql-codegen/cli": "6.1.0",
    "@graphql-codegen/client-preset": "5.2.1",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-urql": "^4.0.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.2",
    "stylelint": "^16.26.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "stylelint-prettier": "^5.0.3",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^4.9.5",
    "web-vitals": "^5.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.*": [
      "prettier-eslint --write",
      "eslint",
      "stylelint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
